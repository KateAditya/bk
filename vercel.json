{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/index.js"
    },
    { "src": "/createOrder", "methods": ["POST"], "dest": "/index.js" },
    { "src": "/verifyPayment", "methods": ["POST"], "dest": "/index.js" },
    {
      "src": "/(.*)",
      "dest": "/index.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
